{
  "name": "ID extractor",
  "description": "Trích xuất ID của bài viết",
  "version": "2.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "tabs", "scripting", "notifications"],
  "host_permissions": ["https://*.facebook.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "/images/binocular.png"
    },
    "default_title": "Click để lấy id bài viết"
  },
  "icons": {
    "16": "/images/binocular.png"
  }
}
